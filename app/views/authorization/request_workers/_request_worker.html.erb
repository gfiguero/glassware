<div id="<%= dom_id request_worker %>">
  <p>
    <strong>Request:</strong>
    <%= request_worker.request_id %>
  </p>

  <p>
    <strong>Worker:</strong>
    <%= request_worker.worker_id %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= request_worker.created_at %>
  </p>

  <p>
    <strong>Updated at:</strong>
    <%= request_worker.updated_at %>
  </p>

  <p>
    <strong>Worker profile:</strong>
    <%= request_worker.worker_profile_id %>
  </p>

  <p>
    <strong>Owner type:</strong>
    <%= request_worker.owner_type %>
  </p>

  <p>
    <strong>Workerable:</strong>
    <%= request_worker.workerable_id %>
  </p>

  <p>
    <strong>Workerable type:</strong>
    <%= request_worker.workerable_type %>
  </p>

</div>
